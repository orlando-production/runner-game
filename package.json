{
  "name": "runner-game",
  "version": "1.0.0",
  "description": "Educational project game for Yandex",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "webpack serve --config webpack.dev.config.ts",
    "build": "webpack build --config webpack.prod.config.ts",
    "test": "jest",
    "prepare": "husky install",
    "prod": "NODE_ENV=production webpack --mode=production --config webpack.prod.config.ts",
    "serve": "npm run prod && serve -s dist",
    "linter": "eslint . --ext js,jsx,ts,tsx",
    "linter:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "check:ts": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all --parallel linter check:ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orlando-production/runner-game.git"
  },
  "keywords": [
    "game",
    "runner"
  ],
  "author": "orlando-production",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/orlando-production/runner-game/issues"
  },
  "homepage": "https://github.com/orlando-production/runner-game#readme",
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@babel/runtime": "7.16.0",
    "@emotion/react": "11.5.0",
    "@emotion/styled": "11.3.0",
    "@iconify/react": "3.0.1",
    "@mui/material": "5.0.6",
    "@mui/types": "7.0.0",
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.10",
    "@types/react-redux": "7.1.20",
    "@types/redux": "3.6.0",
    "@types/webpack": "4.4.17",
    "@types/webpack-dev-server": "4.3.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "axios": "0.24.0",
    "babel-loader": "8.2.3",
    "css-loader": "6.5.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "25.2.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.2",
    "jest": "27.3.1",
    "path": "0.12.7",
    "postcss-loader": "6.2.0",
    "postcss-nested": "5.0.6",
    "postcss-preset-env": "6.7.0",
    "react-scripts": "4.0.3",
    "style-loader": "3.3.1",
    "stylelint": "14.0.1",
    "stylelint-config-standard": "23.0.0",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "typescript": "4.4.4",
    "webpack": "5.61.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.4.0",
    "react-cookie": "4.1.1"
  },
  "dependencies": {
